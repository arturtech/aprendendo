#include <iostream>

using namespace std;

int main()
{
    int t, n, i, ponto, pontos_soma;
    char nome[20];
    cin >> t >> n;
    if(t==0){brake;}
    else{
        pontos_soma=0;
        for(i=0; i<t; i++){
            cin >> nome >> ponto;
            pontos_soma += ponto;
        }
    }    
        cout << (n*t)-pontos_soma << endl;
    return 0;
}
